{
  "name": "warroom-3.0-ui",
  "private": true,
  "version": "1.0.3",
  "type": "module",
  "overrides": {
    "rollup": "4.13.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --log-level=error",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "health:check": "echo 'üè• Running health check...' && echo 'üìù Code format check...' && npm run format:check && echo '‚úÖ Format check passed!' && echo 'üî® Build check...' && npm run build && echo '‚úÖ Build successful!' && echo '‚úÖ Health check complete!'",
    "health:full": "echo 'üè• Running full health check...' && npm run health:check && echo 'üìã Running lint check...' && (npm run lint --silent || echo '‚ö†Ô∏è  Lint warnings found (non-blocking)') && echo 'üîç Running type check...' && (npm run type-check --silent || echo '‚ö†Ô∏è  Type errors found (non-blocking)') && npm run build --silent && npm run test:coverage --silent && echo '‚úÖ Full health check passed!'",
    "health:ci": "npm run format:check && npm run lint && npm run type-check && npm run test && npm run build",
    "test:ci": "vitest run --reporter=verbose",
    "test:stable": "vitest run --testNamePattern=\"(DashboardChart|useReducedMotion|ErrorBoundary)\"",
    "monitor": "node scripts/monitor-site.js",
    "test:health": "playwright test tests/monitoring/live-site-health.spec.js --project=monitoring",
    "test:comprehensive": "playwright test tests/monitoring/comprehensive-health-check.spec.js --project=monitoring",
    "test:all-monitoring": "playwright test tests/monitoring --project=monitoring",
    "playwright:install": "playwright install chromium",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level moderate",
    "quality:check": "npm run lint && npm run type-check && npm run test:stable",
    "ci:full": "npm run quality:check && npm run test:coverage && npm run security:check",
    "build-disabled": "echo Build disabled - using pre-built files with hash index-9bcae6e1",
    "check:process": "! grep -r 'process\\.' src/ --include='*.ts' --include='*.tsx' --include='*.js' --include='*.jsx' --exclude-dir='__tests__' --exclude-dir='dist' --exclude='*.test.*' --exclude='*.spec.*' | grep -v 'test' | grep -v 'spec' | grep -v '// Browser' || echo '‚úÖ No process references in frontend code'",
    "check:built": "! grep -l 'process\\.' dist/assets/*.js 2>/dev/null || (echo '‚ùå ERROR: Found process references in built files!' && exit 1)",
    "build:safe": "npm run check:process && npm run build && npm run check:built"
  },
